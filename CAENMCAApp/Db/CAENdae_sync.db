
record(seq, "$(P)$(Q)SYNCDAE:BEGIN")
{
	field(DOL1, "$(P)$(Q)RUNNUMBER")
	field(LNK1, "$(P)DAE:RUNNUMBER:SET PP")
	field(DO2, "1")
	field(LNK2, "$(P)DAE:BEGINRUN PP")
}
	
record(seq, "$(P)$(Q)SYNCDAE:ABORT")
{
	field(DO1, "1")
	field(LNK1, "$(P)DAE:ABORTRUN PP")
}
	
record(seq, "$(P)$(Q)SYNCDAE:END")
{
	field(DO1, "1")
	field(LNK1, "$(P)DAE:ENDRUN PP")
#	field(DOL2, "$(P)$(Q)RUNNUMBER")
#	field(LNK2, "$(P)DAE:RUNNUMBER:SET PP")
}

record(lso, "$(P)$(Q)SYNCDAE:TITLE")
{
    field(OMSL, "closed_loop")
    field(SIZV, "512")
    field(DOL, "$(P)DAE:TITLE:SP CP")
    field(OUT, "$(P)$(Q)TITLE:SP PP")
}

record(lso, "$(P)$(Q)SYNCDAE:COMMENTS")
{
    field(OMSL, "closed_loop")
    field(SIZV, "512")
    field(DOL, "$(P)PARS:SAMPLE:COMMENTS:SP CP")
    field(OUT, "$(P)$(Q)COMMENTS:SP PP")
}
